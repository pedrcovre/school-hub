│   │   └── AdminRequestView.jsx
│   │
│   ├── resources/              # Recursos para alunos
│   │   └── StudentResourcesPage.jsx
│   │
│   └── dashboard/              # Telas principais
│       ├── AdminDashboard.jsx
│       └── StudentDashboard.jsx
│
├── layouts/                    # Layouts com navbar, etc.
│   └── MainLayout.jsx
│
├── hooks/                      # Hooks personalizados
│   └── useFetchRequests.js
│
├── services/                   # Funções de acesso a API
│   ├── authService.js
│   ├── requestService.js
│   └── resourceService.js
│
├── routes/                     # Rotas centralizadas
│   └── AppRoutes.jsx
│
├── styles/                     # Estilos globais ou themes
│   └── globals.css
│
├── App.jsx                     # Componente raiz
└── main.jsx                    # Ponto de entrada (ReactDOM)